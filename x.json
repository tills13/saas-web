{
  "contentBase": "./build",
  "historyApiFallback": true,
  "host": "0.0.0.0",
  "inline": true,
  "disableHostCheck": true,
  "port": 8000,
  "proxy": {
    "/api": {
      "target": "http://localhost:3000",
      "secure": false
    },
    "/static": {
      "target": "http://localhost:3000",
      "secure": false
    },
    "/graphql": {
      "target": "http://localhost:3000",
      "secure": false
    },
    "/socket.io": {
      "target": "http://127.0.0.1:3001",
      "secure": false
    }
  },
  "watchOptions": { "poll": true },
  "stats": { "colors": true }
}